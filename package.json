  "main": "./dist/bundles/streamdataio.min.js",
  "typings": "./dist/bundles/streamdataio.d.ts",
  "scripts": {
    "clean": "shx rm -rf dist",
    "test": "jest",
    "test:watch": "jest --clearCache && jest --watch",
    "test:coverage": "jest --runInBand --coverage",
    "test:debug": "node --inspect-brk ./node_modules/.bin/jest --runInBand",
    "build:docs": "typedoc --options ./build/typedoc.json ./src",
    "build:js": "tsc && tsc -m es6 --outDir ./dist/es6",
    "build:node": "webpack --config=./build/webpack-node.config.js",
    "build:web": "webpack --config=./build/webpack-web.config.js",
    "build:types": "yarn build:js && dts-bundle --configJson ./build/dts-bundle.json",
    "build": "yarn clean && yarn test:coverage && yarn build:node && yarn build:web && yarn build:types && yarn build:docs"
  },
  "devDependencies": {
    "@types/core-js": "^0.9.46",
    "@types/jest": "^22.2.3",
    "@types/node": "^7.0.68",
    "awesome-typescript-loader": "^5.2.0",
    "dts-bundle": "^0.7.2",
    "eventsourcemock": "^2.0.0",
    "jest": "^23.1.0",
    "jest-sonar-reporter": "^2.0.0",
    "shx": "^0.2.2",
    "source-map-loader": "^0.2.1",
    "ts-jest": "^23.1.3",
    "tslint": "^5.11.0",
    "tslint-loader": "^3.6.0",
    "typedoc": "^0.11.1",
    "typescript": "^2.7.2",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "webpack": "^4.16.3",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5",
    "webpack-merge": "^4.1.4",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "core-js": "^2.5.7",
    "eventsource": "github:streamdataio/eventsource-js#v1.0.6",
    "eventsource-polyfill": "github:streamdataio/EventSource#v1.0.4",
    "yarn": "^1.9.4"
  }
}
