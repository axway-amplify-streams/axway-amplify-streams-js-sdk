{
  "name": "streamdataio-js-sdk",
  "version": "2.0.2",
  "description": "Javascript SDK for Streamdata.io",
  "copyrights": " 2015",
  "homepage": "https://github.com/streamdataio/streamdataio-js-sdk",
  "repository": {
    "type": "git",
    "url": "https://github.com/streamdataio/streamdataio-js-sdk"
  },
  "keywords": [
    "streamdata",
    "streamdata.io",
    "push",
    "sse",
    "server-sent-event"
  ],
